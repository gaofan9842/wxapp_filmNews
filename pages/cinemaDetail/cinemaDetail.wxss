.fd-favorite {
    margin-left: 500rpx;
    box-sizing: border-box;    
    z-index: 99;
    
    left: 120rpx;
    width: 50rpx;
    height: 50rpx;
    padding: 10rpx 30rpx;
    padding-left: 100rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url(data:image/png;base64,);
    background-size: 40rpx;
    color:#ffff;
    transition: left .3s, color .3s, background-image .3s;
}

.fd-favorite-active {
    left: 120rpx;
    width: 50rpx;
    height: 50rpx;
    background-image: url(data:image/png;base64,);
    color: #f4ea2a;
}